{
  "name": "wotan",
  "version": "0.0.0",
  "description": "Pluggable TypeScript and JavaScript linter",
  "main": "bin/main.js",
  "scripts": {
    "compile": "tsc -p .",
    "lint": "run-p lint:tslint lint:wotan",
    "lint:tslint": "tslint -p .",
    "lint:wotan": "node lib/cli lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajafff/wotan.git"
  },
  "keywords": [
    "lint",
    "ts",
    "js",
    "linter",
    "code",
    "style",
    "quality",
    "typescript",
    "javascript"
  ],
  "author": "Klaus Meinhardt",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ajafff/wotan/issues"
  },
  "homepage": "https://github.com/ajafff/wotan#readme",
  "devDependencies": {
    "@types/glob": "^5.0.33",
    "@types/js-yaml": "^3.10.1",
    "@types/json5": "0.0.29",
    "@types/minimatch": "^3.0.1",
    "@types/multimatch": "^2.1.2",
    "@types/node": "^8.0.53",
    "@types/resolve": "0.0.4",
    "npm-run-all": "^4.1.2",
    "tslint": "^5.8.0",
    "tslint-consistent-codestyle": "^1.10.1",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "glob": "^7.1.2",
    "import-local": "^0.1.1",
    "js-yaml": "^3.10.0",
    "json5": "^0.5.1",
    "minimatch": "^3.0.4",
    "multimatch": "^2.1.0",
    "resolve": "^1.5.0",
    "tslib": "^1.8.0",
    "tsutils": "^2.13.0"
  },
  "peerDependencies": {
    "typescript": "^2.4.1"
  }
}
