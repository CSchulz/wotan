{
  "extends": [
    "config:base",
    ":semanticCommitsDisabled",
    ":preserveSemverRanges",
    ":prNotPending",
    ":automergeMinor",
    ":automergeDigest",
    ":automergeBranchPush",
    ":automergeRequireAllStatusChecks",
    ":maintainLockFilesWeekly",
    "group:definitelyTyped",
    "group:linters"
  ],
  "timezone": "Europe/Berlin",
  "schedule": [
    "after 10pm",
    "before 5am"
  ],
  "lockFileMaintenance": {
    "automerge": true
  },
  "node": {
    "enabled": true,
    "supportPolicy": ["current"]
  },
  "docker": {
    "enabled": false,
    "extends": "schedule:weekly"
  }
}
